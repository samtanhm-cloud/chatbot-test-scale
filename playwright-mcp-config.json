{
  "browser": {
    "channel": "chrome",
    "headless": true,
    "args": [
      "--headless=new",
      "--no-sandbox",
      "--disable-setuid-sandbox",
      "--disable-dev-shm-usage",
      "--disable-background-networking",
      "--disable-default-apps",
      "--no-first-run",
      "--disable-features=TranslateUI"
    ]
  },
  "defaultTimeout": 30000,
  "launchOptions": {
    "channel": "chrome",
    "headless": true,
    "args": [
      "--headless=new",
      "--no-sandbox",
      "--disable-setuid-sandbox",
      "--disable-dev-shm-usage"
    ]
  },
  "contextOptions": {
    "storageState": "auth/draftr-session.json"
  }
}

